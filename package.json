		"react-redux": "^8.1.3",
		"react-router-dom": "^6.20.1",
		"react-scripts": "5.0.1",
		"react-sweet-state": "^2.7.1",
		"react-switch": "^7.0.0",
		"react-toastify": "9.1.3",
