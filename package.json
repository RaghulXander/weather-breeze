		"@types/node": "^16.7.13",
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"axios": "^1.6.2",
		"postcss-cli": "^10.1.0",
		"react": "^18.2.0",
		"react-awesome-slider": "^4.1.0",
